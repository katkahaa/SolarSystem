            </ShellContent>
            <ShellContent Title="Mars">
                <Page></Page>
            </ShellContent>
        </Tab>
        <Tab Title="Moons">
            <ShellContent Title="Moon">
                <Page></Page>
            </ShellContent>
            <ShellContent Title="Europa">
                <Page></Page>
            </ShellContent>
        </Tab>
        <Tab Title="Stars">
            <ShellContent Title="Sun">
                <Page></Page>
            </ShellContent>
            <ShellContent Title="Mars">
                <Page></Page>
            </ShellContent>
        </Tab>
